package mx.gob.segob.dgtic.webservices.aplicacionconfig.exception;


/**
 * Excepci&oacute;n lanzada en los procesos de configuraci&oacute;n durante el tiempo de ejecuci&oacute;n.
 */
public class ConfiguracionException extends RuntimeException {

    /**
     * La constante del serialVersionUID.
     */
	private static final long serialVersionUID = 1L;

	
	
	/**
	 * Instancia a una nueva excepci&oacute;n de configuraci&oacute;n
	 *
	 * @param mensaje El mensaje a registrar de la excepci&oacute;n
	 */
	public ConfiguracionException(String mensaje){
        super(mensaje);
    }

    /**
     * Instancia a una nueva excepci&oacute;n de configuraci&oacute;n
     *
     * @param mensaje El mensaje a registrar de la excepci&oacute;n
     * @param cause La excepci&oacute;n causa del error
     */
    public ConfiguracionException(String mensaje, Throwable cause) {
        super(mensaje, cause);
    }
}
