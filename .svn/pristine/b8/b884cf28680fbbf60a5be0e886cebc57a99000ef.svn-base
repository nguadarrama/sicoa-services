package mx.gob.segob.dgtic.comun.transport.dto.auditoria;

import java.util.Date;

/**
 * Clase que transporta la informaci&oacute;n de la auditoria a bitacorizar 
 */
public class AuditoriaBitacoraDto {
	
	/**
	 * El id de la bitacora.
	 */
	private Integer idBitacoraAuditoria;
	
	/**
	 * La fecha en que se audito.
	 */
	private Date fechaAudito;
	
	/**
	 * La clave del usuario.
	 */
	private String cveUsuario;
	
	/**
	 * La URI que se solicito
	 */
	private String uri;
	
	/**
	 * El modulo asociado a la URI
	 */
	private String modulo;
	
	/**
	 * Los parametros de entrada en formato JSON
	 */
	private String parametros;
	
	/**
	 * El estatus del resultado de la peticion (HTTP.status).
	 */
	private int estatusRespuesta;
	
	/**
	 * El tipo de contenido de la respuesta (Content-type)
	 */
	private String contentTypeRespuesta;
	
	/**
	 * La respuesta en formato JSON
	 */
	private String respuesta;
	
	/**
	 * Obtiene id de la bitacora.
	 *
	 * @return El id de la bitacora.
	 */
	public Integer getIdBitacoraAuditoria() {
		return idBitacoraAuditoria;
	}
	
	/**
	 * Asigna id de la bitacora.
	 *
	 * @param idBitacoraAuditoria El id de la bitacora a asignar.
	 */
	public void setIdBitacoraAuditoria(Integer idBitacoraAuditoria) {
		this.idBitacoraAuditoria = idBitacoraAuditoria;
	}
	
	/**
	 * Obtiene fecha en que se audito.
	 *
	 * @return La fecha en que se audito.
	 */
	public Date getFechaAudito() {
		return fechaAudito;
	}
	
	/**
	 * Asigna fecha en que se audito.
	 *
	 * @param fechaAudito La fecha en que se audito.
	 */
	public void setFechaAudito(Date fechaAudito) {
		this.fechaAudito = fechaAudito;
	}
	
	
	/**
	 * Obtiene la URI que se solicito
	 *
	 * @return la URI
	 */
	public String getUri() {
		return uri;
	}
	
	/**
	 * Asigna la uri
	 *
	 * @param uri la URI
	 */
	public void setUri(String uri) {
		this.uri = uri;
	}
	
	/**
	 * Obtiene El modulo asociado a la URI
	 *
	 * @return El modulo asociado a la URI
	 */
	public String getModulo() {
		return modulo;
	}
	
	/**
	 * Asigna el modulo asociado a la URI.
	 *
	 * @param modulo El modulo asociado a la URI
	 */
	public void setModulo(String modulo) {
		this.modulo = modulo;
	}
	
	/**
	 * Obtiene los parametros de entrada.
	 *
	 * @return Los parametros de entrada
	 */
	public String getParametros() {
		return parametros;
	}
	
	/**
	 * Asigna los parametros de entrada.
	 *
	 * @param parametros los parametros de entrada
	 */
	public void setParametros(String parametros) {
		this.parametros = parametros;
	}
	
	/**
	 * Obtiene el estatus respuesta.
	 *
	 * @return el estatus respuesta.
	 */
	public int getEstatusRespuesta() {
		return estatusRespuesta;
	}
	
	/**
	 * Asigna el estatus respuesta.
	 *
	 * @param estatusRespuesta el estatus respuesta.
	 */
	public void setEstatusRespuesta(int estatusRespuesta) {
		this.estatusRespuesta = estatusRespuesta;
	}
	
	/**
	 * Obtiene el content-type de la respuesta.
	 *
	 * @return el content-type de la respuesta.
	 */
	public String getContentTypeRespuesta() {
		return contentTypeRespuesta;
	}
	
	/**
	 * Asigna el content-type de la respuesta.
	 *
	 * @param contentTypeRespuesta el content-type de la respuesta.
	 */
	public void setContentTypeRespuesta(String contentTypeRespuesta) {
		this.contentTypeRespuesta = contentTypeRespuesta;
	}
	
	/**
	 * Obtiene la respuesta.
	 *
	 * @return la respuesta
	 */
	public String getRespuesta() {
		return respuesta;
	}
	
	/**
	 * Asigna la respuesta
	 *
	 * @param respuesta la respuesta
	 */
	public void setRespuesta(String respuesta) {
		this.respuesta = respuesta;
	}


	/**
	 * Obtiene la clave de usuario.
	 *
	 * @return la clave de usuario
	 */
	public String getCveUsuario() {
		return cveUsuario;
	}
	
	/**
	 * Asigna la clave de usuario
	 *
	 * @param cveUsuario la clave de usuario
	 */
	public void setCveUsuario(String cveUsuario) {
		this.cveUsuario = cveUsuario;
	}
	
	
}
