package mx.gob.segob.dgtic.comun.util.resteasy.token.dto;

import java.util.Date;

import mx.gob.segob.dgtic.comun.util.resteasy.token.constant.SubjectTokenEnum;

/**
 * Clase que contiene la informaci&oacute;n que conforma el token de seguridad
 */
public class TokenDto {
    
    /**
     * El id token.
     */
    private String idToken;
    
    /**
     * El emisor.
     */
    private String emisor;
    
    /**
     * El solicitante.
     */
    private String solicitante;
    
    /**
     * El asunto.
     */
    private SubjectTokenEnum asunto;
    
    /**
     * La fecha en que expira token.
     */
    private Date fechaExpiraToken;
    
    /**
     * La informaci&oacute; adicional.
     */
    private InformacionAdicionalDTO informacionAdicional;

    /**
     * Obtiene el id token.
     *
     * @return el id token
     */
    public String getIdToken() {
        return idToken;
    }

    /**
     * Asigna el id token.
     *
     * @param idToken El id token
     */
    public void setIdToken(String idToken) {
        this.idToken = idToken;
    }

    /**
     * Obtiene el emisor.
     *
     * @return el emisor
     */
    public String getEmisor() {
        return emisor;
    }

    /**
     * Asigna el emisor.
     *
     * @param emisor el emisor
     */
    public void setEmisor(String emisor) {
        this.emisor = emisor;
    }

    /**
     * Obtien el solicitante.
     *
     * @return el solicitante
     */
    public String getSolicitante() {
        return solicitante;
    }

    /**
     * Asigna el solicitante.
     *
     * @param solicitante el solicitante
     */
    public void setSolicitante(String solicitante) {
        this.solicitante = solicitante;
    }

    /**
     * Obtiene el asunto.
     *
     * @return el asunto
     */
    public SubjectTokenEnum getAsunto() {
        return asunto;
    }

    /**
     * Asigna el  asunto.
     *
     * @param asunto el asunto
     */
    public void setAsunto(SubjectTokenEnum asunto) {
        this.asunto = asunto;
    }


    /**
     * Obtiene la informaci&oacute;n adicional.
     *
     * @return la informaci&oacute;n adicional
     */
    public InformacionAdicionalDTO getInformacionAdicional() {
        return informacionAdicional;
    }

    /**
     * Asigna la informaci&oacute;n adicional.
     *
     * @param informacionAdicional la informaci&oacute;n adicional
     */
    public void setInformacionAdicional(InformacionAdicionalDTO informacionAdicional) {
        this.informacionAdicional = informacionAdicional;
    }

    /**
     * Obtiene la fecha en que expira token.
     *
     * @return la fecha en que expira token
     */
    public Date getFechaExpiraToken() {
        return fechaExpiraToken;
    }

    /**
     * Asigna la fecha en que expira token.
     *
     * @param fechaExpiraToken la fecha en que expira token
     */
    public void setFechaExpiraToken(Date fechaExpiraToken) {
        this.fechaExpiraToken = fechaExpiraToken;
    }
}
